# mount point        fstype                device                        [device2]
/cache             ext4       /dev/block/platform/ff0f0000.dwmmc/by-name/cache
/system            ext4       /dev/block/platform/ff0f0000.dwmmc/by-name/system
/data              ext4       /dev/block/platform/ff0f0000.dwmmc/by-name/userdata       flags=encryptable=/dev/block/platform/ff0f0000.dwmmc/by-name/encrypt
/boot              emmc       /dev/block/platform/ff0f0000.dwmmc/by-name/boot
/recovery          emmc       /dev/block/platform/ff0f0000.dwmmc/by-name/recovery
/misc              emmc       /dev/block/platform/ff0f0000.dwmmc/by-name/misc
/external_sd       vfat       /dev/block/mmcblk1p1  /dev/block/mmcblk1
/usb-otg           vfat       /dev/block/sda1  /dev/block/sda                       flags=storage;wipeingui;removable
